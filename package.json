{
  "name": "simplePWA",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/types/index.d.ts",
  "license": "ISC",
  "scripts": {
    "build": "tsc && rollup -c rollup.config.ts",
    "test": "jest --color",
    "test:coverage": "jest --coverage",
    "changeset": "changeset",
    "release:version": "changeset version",
    "release:publish": "yarn build && changeset publish",
    "prettier:fix": "prettier --ignore-unknown --write",
    "prettier:check": "prettier --check .",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "lint:scripts": "eslint ./src --color",
    "test_threshold": "jest --coverage --coverageReporters=text --coverageReporters='json-summary'",
    "test:full-coverage": "jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}'",
    "create-pwa": "node ./src/cli/pwa/createPWA.js",
    "create-sw": "node ./src/cli/serviceWorker/createServiceWorker.js"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.15.17",
    "@types/react": "^18.3.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.8.8",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-node-externals": "^4.1.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "stylelint": "^14.13.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "idb": "^7.1.1",
    "inquirer": "^12.6.1",
    "jest-canvas-mock": "^2.5.2",
    "uuid": "^10.0.0"
  }
}
